<?xml version='1.0' encoding='utf-8'?>
<widget id="lombalgia.fisioterapia" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>LBP Screening</name>
    <description>
      Aplicativo de Apoio ao diagn√≥stico de Lombalgia.
    </description>
    <author email="matheuskaiya2@gmail.com">
        Matheus Ryuki Medeiros Kaiya
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="mdpi" src="www/img/logo.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="LoadUrlTimeoutValue" value="100000" />
</widget>
